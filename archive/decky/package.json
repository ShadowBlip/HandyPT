    "decky",
    "plugin",
    "Aya",
    "Neo",
    "AyaNeo",
    "OXP",
    "OneXPlayer",
    "AOKZOE",
    "GPD",
    "handheld",
    "TDP",
    "power"
  ],
  "author": "Derek Clark <derekjohn.clark@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ShadowBlip/HandyPT/issues"
  },
  "homepage": "https://github.com/ShadowBlip/HandyPT#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "rollup": "^2.77.1",
